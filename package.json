  "scripts": {
    "start": "nx run-many --target=serve --all --maxParallel=100 ",
    "build": "nx build",
    "dev": "nx run-many --parallel --target=serve --projects=api,dashboard,website",
    "dev:api": "nx run-many --parallel --target=serve --projects=api",
    "dev:dashboard": "nx run-many --parallel --target=serve --projects=dashboard",
    "dev:website": "nx run-many --parallel --target=serve --projects=website",
    "test": "npx nx run-many --all --target=test --parallel",
    "test:e2e:dashboard": "nx e2e dashboard-e2e",
    "test:e2e:website": "nx e2e website-e2e",
    "test:e2e:dashboard:watch": "nx e2e dashboard-e2e --watch",
    "test:e2e:website:watch": "nx e2e website-e2e --watch",
    "lint": "nx workspace-lint && npx nx run-many --all --target=lint --parallel",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start:ui": "nx run ui:serve",
    "start:api": "nx run api:serve",
    "create-entities-index": "ts-node --pretty --project=tools/tsconfig.tools.json tools/create-entities-index.ts",
    "compodoc:watch": "npx @compodoc/compodoc -p tsconfig.base.json -w -s",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d apps/api/src/config/datasource.ts",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate apps/api/src/migrations/Migration -d apps/api/src/config/datasource.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d apps/api/src/config/datasource.ts"
  },
  "private": true,
